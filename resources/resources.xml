<resources>
	
    <string id="AppName">CleanSimpleAnalog</string>
	<string id="handsOutline">Outline of Hands</string>
	<string id="secondHandType">Second Hand Type</string>
	<string id="showAllHourNumbers">Show all Hour Numbers</string>
	
	<string id="sweep">Sweep</string>
	<string id="dial">Dial</string>
	
	<string id="none">None</string>
	<string id="white">White</string>
	<string id="lightGray">Light Gray</string>
	<string id="darkGray">Dark Gray</string>
	<string id="black">Black</string>
	<string id="red">Red</string>
	<string id="darkRed">Dark Red</string>
	<string id="orange">Orange</string>
	<string id="yellow">Yellow</string>
	<string id="green">Green</string>
	<string id="darkGreen">Dark Green</string>
	<string id="blue">Blue</string>
	<string id="darkBlue">Dark Blue</string>
	<string id="purple">Purple</string>
	<string id="pink">Pink</string>
	
	<properties>
	    <property id="appVersion" type="string">0.0.1</property>
	    <property id="handsOutline" type="number">0</property>
	    <property id="secondHandType" type="number">0</property>
	    <property id="showAllHourNumbers" type="boolean">false</property>
	</properties>
	
	<settings>
	    <setting propertyKey="@Properties.appVersion" title="@Strings.AppName">
	        <settingConfig type="alphaNumeric" readonly="true" />
	    </setting>
	
	    <setting propertyKey="@Properties.handsOutline" title="@Strings.handsOutline">
	        <settingConfig type="list">
	            <listEntry value="0">@Strings.none</listEntry>
	            <listEntry value="1">@Strings.white</listEntry>
	            <listEntry value="2">@Strings.lightGray</listEntry>
	            <listEntry value="3">@Strings.darkGray</listEntry>
	            <listEntry value="4">@Strings.black</listEntry>
	            <listEntry value="5">@Strings.red</listEntry>
	            <listEntry value="6">@Strings.darkRed</listEntry>
	            <listEntry value="7">@Strings.orange</listEntry>
	            <listEntry value="8">@Strings.yellow</listEntry>
	            <listEntry value="9">@Strings.green</listEntry>
	            <listEntry value="10">@Strings.darkGreen</listEntry>
	            <listEntry value="11">@Strings.blue</listEntry>
	            <listEntry value="12">@Strings.darkBlue</listEntry>
	            <listEntry value="13">@Strings.purple</listEntry>
	            <listEntry value="14">@Strings.pink</listEntry>
	        </settingConfig>
	    </setting>
	
	    <setting propertyKey="@Properties.secondHandType" title="@Strings.secondHandType">
	        <settingConfig type="list">
	            <listEntry value="0">@Strings.sweep</listEntry>
	            <listEntry value="1">@Strings.dial</listEntry>
	        </settingConfig>
	    </setting>

	    <setting propertyKey="@Properties.showAllHourNumbers" title="@Strings.showAllHourNumbers">
	        <settingConfig type="boolean" />
	    </setting>
	</settings>
	
	<fonts>
        <font id="id_font_hours_digits" filename="fonts/hoursDigits.fnt" />
        <font id="id_font_seconds_dials" filename="fonts/secondsDials.fnt" />
        	</fonts>
	
    <bitmap id="LauncherIcon" filename="images/cleansimpleanalog.png" />
    <bitmap id="DoNotDisturbIcon" filename="images/donotdisturb.png">
        <palette>
            <color>000000</color>
        </palette>
    </bitmap>
</resources>